{
  "name": "<%= packageName %>",
  "version": "0.1.0",
  "description": "<%= humanName %>",
  "main": "lib/<%= className %>.js",
  "homepage": "https://github.com/<%= gitUser %>/<%= repoName %>",
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= gitUser %>/<%= repoName %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= gitUser %>/<%= repoName %>/issues"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV=development webpack-dev-server --progress --inline --hot",
    "build": "rm ./lib/* && NODE_ENV=production rollup -c",
    "test": "jest"
  },
  "keywords": [
    "react",
    "component"
  ],
  "author": "<%= author %>",
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.23.1",
    "eslint-plugin-react": "^5.2.2",
    "html-webpack-plugin": "^2.21.0",
    "jest-cli": "^12.1.1",
    "rollup": "^0.31.2",
    "rollup-plugin-babel": "^2.5.1",
    "rollup-plugin-stylus-css-modules": "^1.0.5",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "webpack": "^1.13.1",
    "webpack-build-notifier": "^0.1.8",
    "webpack-dev-server": "^1.14.1"
  },
  "jest": {
    "testPathDirs": [
      "src"
    ],
    "testRegex": "/.*\\.spec\\.js$"
  }
}
